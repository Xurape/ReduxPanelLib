{
    "name": "xuap/reduxpanel_laravel",
    "description": "Use ReduxPanel API right inside your laravel project with this package.",
    "version": "1.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Xuap\\ReduxPanelLib\\": "src/"
        }
    },
    "autoload-dev": { },
    "extra": {
        "laravel": {
            "providers": [
                "Xuap\\ReduxPanelLib\\ReduxPanelServiceProvider"
            ],
            "aliases": {
                "ReduxPanel": "Xuap\\ReduxPanelLib\\Facades\\ReduxPanel"
            }
        }
    },
    "authors": [
        {
            "name": "Jo√£o Ferreira",
            "email": "joaoferreira@reduxpanel.com",
            "role": "Developer",
            "homepage": "https://reduxpanel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.38",
        "illuminate/support": "^9.38"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true,  
      "preferred-install": "dist",
      "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }  
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    }
}
